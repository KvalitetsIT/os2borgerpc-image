      <script type="text/javascript">
        /* <![CDATA[ */
        function validate() {
          var term_link = true;
          if ( document.getElementById('term_link') && document.getElementById('term_link').value == '' )
            term_link = false;

          FixTrunc(document.f.term1);
          FixTrunc(document.f.term2);
          FixTrunc(document.f.term3);
          FixTrunc(document.f.term4);

          var sprog, term_sprog, mat, term_mats, i;

          selected_sprog = false;
          term_sprog = document.getElementsByTagName('input');
          for ( i=0, sprog; sprog=term_sprog.item(i++); )
            if ( sprog.name == 'term_sprog[]' && sprog.checked && sprog.value != '' )
              selected_sprog = true;

          selected_mat = false;
          term_mats = document.getElementsByTagName('input');
          for ( i=0, mat; mat=term_mats.item(i++); )
            if ( mat.name == 'term_mat[]' && mat.checked && mat.value != '' )
              selected_mat = true;

          if ( ( document.f.term1.value == "" || document.f.term1.value == "?" ) &&
               ( document.f.term2.value == "" || document.f.term2.value == "?" ) &&
               ( document.f.term3.value == "" || document.f.term3.value == "?" ) &&
               ( document.f.term4.value == "" || document.f.term4.value == "?" ) &&
               term_link &&
               ! selected_mat &&
               ! selected_sprog ) {
            alert("Du skal søge på et eller andet");
            document.f.term4.focus();
            return false;
          }

          return true;
        }
        /* ]]> */
      </script>

  
